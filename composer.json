{
    "name": "m-s/crud",
    "type": "crud",
    "description": "Crud Generator",
    "keywords": ["m-s", "crud"],
    "homepage": "https://github.com/sulaimaan46/Crud_generator_package",
    "license": "MIT",
    "authors": [
        {
            "name": "sulaimaan46",
            "email": "sulaimaansulai46@gmail.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.2",
        "illuminate/support" : "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "HP\\CrudGenrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HP\\CrudGenrator\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HP\\CrudGenrator\\UserAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"

}
